---
import "../styles/form.css"

interface Props {
  id: string;
}

const { id } = Astro.props
---

<script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/embed/v2.js"></script>
<script is:inline define:vars={{id}}>
  hbspt.forms.create({
    region: "na1",
    portalId: "6604339",
    formId: id
  });
</script>
